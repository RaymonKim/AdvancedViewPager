<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AdvancedViewPager">
        <attr name="avp_useInfiniteScroll" format="boolean" />
        <attr name="avp_rollingInterval" format="integer" />
        <attr name="avp_rollingSpeed" format="integer" />
        <attr name="avp_rollingMode">
            <flag name="none" value="0" />
            <flag name="startToEnd" value="1" />
            <flag name="endToStart" value="2" />
        </attr>

        <attr name="android:orientation"/>
    </declare-styleable>
</resources>